{
  "name": "nutrina",
  "version": "1.0.0",
  "description": "Voice-Powered Diet Tracking Assistant",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && npm run copy-assets",
    "copy-assets": "node -e \"require('fs').cpSync('public', 'dist/public', {recursive: true, force: true})\"",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:nodemon": "nodemon --exec ts-node src/server.ts",
    "test": "jest",
    "test:tts": "npm run build && node scripts/test-google-tts.js",
    "test:realtime": "node scripts/test-realtime.js",
    "test:intent-builder-phase1": "node scripts/test-intent-builder-phase1.js",
    "test:intent-builder-api": "node scripts/test-intent-builder-api.js",
    "create-chatagent-db": "node scripts/create-chatagent-db.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "voice",
    "diet",
    "tracking",
    "ai",
    "websocket",
    "webrtc",
    "realtime",
    "nutrition"
  ],
  "author": "Nutrina Team",
  "license": "MIT",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.4.1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/text-to-speech": "^5.0.0",
    "@google/generative-ai": "^0.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "langchain": "^0.1.0",
    "mongodb": "^6.17.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "openai": "^5.8.2",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
